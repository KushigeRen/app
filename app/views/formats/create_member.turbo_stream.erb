<%= turbo_stream.append "members" do %>
    <%= render partial: "formats/member",locals: { member: @member } %>
<% end %>
<%= turbo_stream.update Member.new, "" %>
<%= turbo_stream.replace "member_select" do %>
    <% members = get_members_info(@member) %>
    <% @search = get_search_info(@member) %>
    <%= render partial: "formats/member_select",locals: { members: members } %>
<% end %>
<%= turbo_stream_flash %>
